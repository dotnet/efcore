<Project>
  <PropertyGroup Label="Version settings">
    <VersionPrefix>3.1.0</VersionPrefix>
    <PreReleaseVersionLabel>alpha1</PreReleaseVersionLabel>
    <IncludeSourceRevisionInInformationalVersion>False</IncludeSourceRevisionInInformationalVersion>
  </PropertyGroup>
  <PropertyGroup Label="Arcade settings">
    <!-- Opt-in to using the ref assembly version bundled with Arcade. -->
    <UsingToolNetFrameworkReferenceAssemblies>True</UsingToolNetFrameworkReferenceAssemblies>
    <UsingToolXliff>False</UsingToolXliff>
  </PropertyGroup>
  <PropertyGroup Label="Dependencies from nuget.org">
    <CastleCorePackageVersion>4.2.1</CastleCorePackageVersion>
    <MicrosoftAzureCosmosPackageVersion>3.0.0</MicrosoftAzureCosmosPackageVersion>
    <MicrosoftCodeAnalysisCSharpPackageVersion>2.8.0</MicrosoftCodeAnalysisCSharpPackageVersion>
    <MicrosoftCodeAnalysisCSharpWorkspacesPackageVersion>2.8.0</MicrosoftCodeAnalysisCSharpWorkspacesPackageVersion>
    <mod_spatialitePackageVersion>4.3.0.1</mod_spatialitePackageVersion>
    <NetTopologySuitePackageVersion>2.0.0</NetTopologySuitePackageVersion>
    <NetTopologySuiteIOSpatiaLitePackageVersion>2.0.0</NetTopologySuiteIOSpatiaLitePackageVersion>
    <NetTopologySuiteIOSqlServerBytesPackageVersion>2.0.0</NetTopologySuiteIOSqlServerBytesPackageVersion>
    <SQLitePCLRawBundleESqlite3PackageVersion>2.0.0</SQLitePCLRawBundleESqlite3PackageVersion>
    <SQLitePCLRawBundleESqlcipherPackageVersion>2.0.0</SQLitePCLRawBundleESqlcipherPackageVersion>
    <SQLitePCLRawCorePackageVersion>2.0.0</SQLitePCLRawCorePackageVersion>
    <StyleCopAnalyzersPackageVersion>1.1.1-beta.61</StyleCopAnalyzersPackageVersion>
    <BenchmarkDotNetPackageVersion>0.11.3</BenchmarkDotNetPackageVersion>
    <MicrosoftDataSqlClientPackageVersion>1.0.19239.1</MicrosoftDataSqlClientPackageVersion>
  </PropertyGroup>
  <PropertyGroup Label="Dependencies from aspnet/Extensions">
    <MicrosoftExtensionsCachingMemoryPackageVersion>3.1.0-alpha1.19457.6</MicrosoftExtensionsCachingMemoryPackageVersion>
    <MicrosoftExtensionsConfigurationPackageVersion>3.1.0-alpha1.19457.6</MicrosoftExtensionsConfigurationPackageVersion>
    <MicrosoftExtensionsConfigurationEnvironmentVariablesPackageVersion>3.1.0-alpha1.19457.6</MicrosoftExtensionsConfigurationEnvironmentVariablesPackageVersion>
    <MicrosoftExtensionsConfigurationJsonPackageVersion>3.1.0-alpha1.19457.6</MicrosoftExtensionsConfigurationJsonPackageVersion>
    <MicrosoftExtensionsDependencyInjectionPackageVersion>3.1.0-alpha1.19457.6</MicrosoftExtensionsDependencyInjectionPackageVersion>
    <MicrosoftExtensionsHostFactoryResolverSourcesPackageVersion>3.1.0-alpha1.19457.6</MicrosoftExtensionsHostFactoryResolverSourcesPackageVersion>
    <MicrosoftExtensionsLoggingPackageVersion>3.1.0-alpha1.19457.6</MicrosoftExtensionsLoggingPackageVersion>
  </PropertyGroup>
  <PropertyGroup Label="Dependencies from dotnet/corefx">
    <MicrosoftCSharpPackageVersion>4.7.0-preview1.19456.20</MicrosoftCSharpPackageVersion>
    <SystemCollectionsImmutablePackageVersion>1.7.0-preview1.19456.20</SystemCollectionsImmutablePackageVersion>
    <SystemComponentModelAnnotationsPackageVersion>4.7.0-preview1.19456.20</SystemComponentModelAnnotationsPackageVersion>
    <SystemDiagnosticsDiagnosticSourcePackageVersion>4.7.0-preview1.19456.20</SystemDiagnosticsDiagnosticSourcePackageVersion>
  </PropertyGroup>
  <PropertyGroup Label="Dependencies from dotnet/core-setup">
    <MicrosoftDotNetPlatformAbstractionsPackageVersion>3.1.0-preview1.19457.3</MicrosoftDotNetPlatformAbstractionsPackageVersion>
    <MicrosoftExtensionsDependencyModelPackageVersion>3.1.0-preview1.19457.3</MicrosoftExtensionsDependencyModelPackageVersion>
    <MicrosoftNETCoreAppRefPackageVersion>3.1.0-preview1.19457.3</MicrosoftNETCoreAppRefPackageVersion>
    <MicrosoftNETCoreAppRuntimewinx64PackageVersion>3.1.0-preview1.19457.3</MicrosoftNETCoreAppRuntimewinx64PackageVersion>
    <NETStandardLibraryRefPackageVersion>2.1.0-preview1.19457.3</NETStandardLibraryRefPackageVersion>
  </PropertyGroup>
  <PropertyGroup Label="Dependencies from dotnet/roslyn">
    <MicrosoftNetCompilersToolsetVersion>3.4.0-beta1-19456-03</MicrosoftNetCompilersToolsetVersion>
  </PropertyGroup>
  <PropertyGroup Label="Dependency version settings">
    <!--
      Win-x64 is used here because we have picked an arbitrary runtime identifier to flow the version of the latest NETCore.App runtime.
      All Runtime.$rid packages should have the same version.
    -->
    <MicrosoftNETCoreAppRuntimeVersion>$(MicrosoftNETCoreAppRuntimewinx64PackageVersion)</MicrosoftNETCoreAppRuntimeVersion>
  </PropertyGroup>
</Project>
