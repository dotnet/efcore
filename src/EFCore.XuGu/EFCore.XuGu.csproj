<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Microsoft XuGu database provider for Entity Framework Core.</Description>
    <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
    <AssemblyName>Microsoft.EntityFrameworkCore.XuGu</AssemblyName>
    <RootNamespace>Microsoft.EntityFrameworkCore.XuGu</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>$(PackageTags);XuGu</PackageTags>
    <ImplicitUsings>true</ImplicitUsings>
    <NoWarn>$(NoWarn);EF9100</NoWarn>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <Nullable>disable</Nullable>
    <!-- Precompiled query is experimental -->
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\EFCore.Relational\EFCore.Relational.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="XuguClient">
      <HintPath>..\..\..\csharp-driver9.0\XGCSClient\bin\Debug\netstandard2.1\XuguClient.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\XGJsonStrings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>XGJsonStrings.Designer.tt</DependentUpon>
    </Compile>
    <Compile Update="Properties\XGStrings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>XGStrings.Designer.tt</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\XGJsonStrings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>XGJsonStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\XGStrings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>XGStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\XGJsonStrings.Designer.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>XGJsonStrings.Designer.cs</LastGenOutput>
    </None>
    <None Update="Properties\XGStrings.Designer.tt">
      <Generator>TextTemplatingFilePreprocessor</Generator>
      <LastGenOutput>XGStrings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>

</Project>
