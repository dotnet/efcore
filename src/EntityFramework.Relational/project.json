{
    "version": "7.0.0-*",
    "description":  "Shared Entity Framework components for relational data stores.",
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "EntityFramework.Core": "7.0.0-*"
    },
    "code": [ "**\\*.cs", "..\\Shared\\*.cs" ],
    "frameworks": {
        "net451": {
            "frameworkAssemblies": {
                "System.Data": ""
            }
        },
        "aspnet50": {
            "frameworkAssemblies": {
                "System.Data": ""
            }
        },
        "aspnetcore50": {
            "dependencies": {
                "Microsoft.CSharp": "4.0.0-beta-*",
                "System.Data.Common": "4.0.0-beta-*",
                "System.Text.RegularExpressions": "4.0.10-beta-*"
            }
        }
    }
}
