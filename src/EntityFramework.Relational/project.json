{
  "version": "7.0.0-rc1-final",
  "description": "Shared Entity Framework components for relational data stores.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "EntityFramework.Core": "7.0.0-rc1-final",
    "System.Diagnostics.DiagnosticSource": "4.0.0-beta-23516"
  },
  "compile": "..\\Shared\\*.cs",
  "namedResource": {
    "EntityFramework.Relational.RelationalStrings": "Properties/RelationalStrings.resx"
  },
  "resource": [
    "Properties/EntityFramework.Relational.rd.xml"
  ],
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.Transactions": "4.0.0.0"
      }
    },
    "dotnet5.4": {
      "dependencies": {
        "System.Data.Common": "4.0.1-beta-23516",
        "System.Text.RegularExpressions": "4.0.11-beta-23516",
        "Microsoft.CSharp": "4.0.1-beta-23516"
      }
    },
    "netcore50": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-beta-23516",
        "System.Data.Common": "4.0.1-beta-23516",
        "System.Dynamic.Runtime": "4.0.11-beta-23516"
      }
    }
  }
}