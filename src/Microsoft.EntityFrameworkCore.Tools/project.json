{
  "version": "1.0.0-preview2-*",
  "description": "Command line tools for Entity Framework Core.",
  "packOptions": {
    "tags": [
      "Entity Framework",
      "Entity Framework Core",
      "EF",
      "Data",
      "O/RM"
    ]
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-ef",
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "compile": {
      "include": "../Shared/*.cs"
    }
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore.Tools.Core": "1.0.0-*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.DotNet.Cli.Utils": "1.0.0-*",
        "Microsoft.DotNet.ProjectModel": "1.0.0-*",
        "Microsoft.EntityFrameworkCore.Tools.Cli": "1.0.0-*",
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        },
        "System.Runtime.InteropServices.RuntimeInformation": "4.0.0-*"
      }
    },
    "net451": {
      "dependencies": {
        "Microsoft.EntityFrameworkCore.Tools.Cli": "1.0.0-*"
      },
      "buildOptions": {
        "outputName": "Microsoft.EntityFrameworkCore.Tools",
        "emitEntryPoint": false
      }
    }
  },
  "packOptions": {
    "files": [
      "lib/",
      "tools/"
    ]
  }
}