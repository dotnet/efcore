{
    "version": "7.0.0-beta7",
    "description": "SQLite data store for Entity Framework.",
    "repository": {
        "type": "git",
        "url": "git://github.com/aspnet/entityframework"
    },
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "EntityFramework.Relational": "7.0.0-beta7",
        "Microsoft.Data.Sqlite": "1.0.0-beta7"
    },
    "compile": "..\\Shared\\*.cs",
    "namedResource": {
        "EntityFramework.Sqlite.Strings": "Properties/Strings.resx"
    },
    "resource": [
        "Properties/EntityFramework.Sqlite.rd.xml"
    ],
    "frameworks": {
        "net45": {},
        "dotnet": {
            "dependencies": {
                "System.IO.FileSystem": "4.0.1-beta-23225",
                "Microsoft.CSharp": "4.0.1-beta-23225"
            }
        }
    }
}