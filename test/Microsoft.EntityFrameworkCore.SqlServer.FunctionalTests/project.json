{
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "copyToOutput": {
      "include": [
        "**/*.sql",
        "config.json"
      ],
      "exclude": [
        "obj/**",
        "bin/**"
      ]
    }
  },
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-*",
    "Microsoft.EntityFrameworkCore.Relational.Specification.Tests": "1.2.0-*",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.2.0-*",
    "Microsoft.EntityFrameworkCore.SqlServer.Design": "1.2.0-*",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.2.0-*",
    "Microsoft.Extensions.Configuration.Json": "1.2.0-*"
  },
  "publishOptions": {
    "include": [
      "**/*.sql",
      "config.json"
    ],
    "exclude": [
      "obj/**",
      "bin/**"
    ]
  },
  "testRunner": "xunit",
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0-*",
          "type": "platform"
        }
      }
    },
    "net451": {}
  }
}